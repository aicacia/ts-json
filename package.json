{
  "name": "@stembord/json",
  "version": "0.1.0",
  "description": "JSON types and helper functions",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/stembord/ts-json.git"
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "module": "./lib/index.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node node_modules/tape/bin/tape ./tests/**/*.test.ts | tap-spec",
    "clean": "rm -rf ./src",
    "format.ts": "prettier-tslint fix '**/*.{ts,tsx}'",
    "format.js": "prettier --write '**/*.{js,jsx,json}' --ignore-path .gitignore",
    "format": "npm run format.ts && npm run format.js",
    "lint": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'"
  },
  "keywords": [
    "json"
  ],
  "author": "Nathan Faucett",
  "license": "(MIT OR Apache-2.0)",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/tape": "^4.2.33",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "camel-case": "^3.0.0",
    "snake-case": "^2.1.0"
  }
}
